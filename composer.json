{
    "name": "by/first-image",
    "description": "A plugin to get first image from Redactor Field.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "first image"
    ],
    "support": {
        "docs": "https://github.com/bhashkar007/first-image/blob/master/README.md",
        "issues": "https://github.com/bhashkar007/first-image/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bhashkar Yadav",
            "homepage": "https://360adaptive.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "by\\firstimage\\": "src/"
        }
    },
    "extra": {
        "name": "First Image",
        "handle": "first-image",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/bhashkar007/first-image/master/CHANGELOG.md",
        "class": "by\\firstimage\\FirstImage"
    }
}
